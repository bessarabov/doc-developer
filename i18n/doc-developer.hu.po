"        $Value = substr( $Value, 0, $Param{ValueMaxChars} ) . '...';\n"
"    }\n"
"    if ( $Param{TitleMaxChars} && length($Title) > $Param{TitleMaxChars} ) {\n"
"        $Title = substr( $Title, 0, $Param{TitleMaxChars} ) . '...';\n"
"    }\n"
"\n"
"    # visszatérési szerkezet létrehozása\n"
"    my $Data = {\n"
"        Value => $Value,\n"
"        Title => $Title,\n"
"    };\n"
"\n"
"    return $Data;\n"
"}\n"

