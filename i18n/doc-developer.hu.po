"    # search tickets\n"
"    return $Self->{TicketObject}->TicketSearch(\n"
"        UserID     => 1,\n"
"        Result     => 'COUNT',\n"
"        Permission => 'ro',\n"
"        Limit      => 100_000_000,\n"
"        %Param,\n"
"    );\n"
"}\n"
msgstr ""
"sub GetStatElement {\n"
"    my ( $Self, %Param ) = @_;\n"
"\n"
"    # jegyek keresése\n"
"    return $Self->{TicketObject}->TicketSearch(\n"
"        UserID     => 1,\n"
"        Result     => 'COUNT',\n"
"        Permission => 'ro',\n"
"        Limit      => 100_000_000,\n"
"        %Param,\n"
"    );\n"
