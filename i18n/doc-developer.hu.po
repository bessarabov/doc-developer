"\n"
"                    my %State = $Self->{StateObject}->StateGet(\n"
"                        Name  => $ID->{Content},\n"
"                        Cache => 1,\n"
"                    );\n"
"                    if ( $State{ID} ) {\n"
"                        $ID->{Content} = $State{ID};\n"
"                    }\n"
"                    else {\n"
"                        $Self->{LogObject}->Log(\n"
"                            Priority => 'error',\n"
"                            Message  => \"Import: Can' find state $ID->{Content}!\"\n"
"                        );\n"
"                        $ID = undef;\n"
"                    }\n"
"                }\n"
"            }\n"
"        }\n"
"    }\n"
"    return \\%Param;\n"
"}\n"
"\n"
