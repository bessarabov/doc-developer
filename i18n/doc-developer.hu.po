msgstr ""
"sub new {\n"
"    my ( $Type, %Param ) = @_;\n"
"\n"
"    my $Self = {};\n"
"    bless( $Self, $Type );\n"
"\n"
"    $Self->{Debug}      = $Param{Debug};\n"
"    $Self->{WorkerName} = 'Worker: Test';\n"
"\n"
"    return $Self;\n"
