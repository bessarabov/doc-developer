"    <Name>Naptár</Name>\n"
"    <Version>0.0.1</Version>\n"
"    <Framework>3.2.x</Framework>\n"
"    <Vendor>OTRS AG</Vendor>\n"
"    <URL>http://otrs.org/</URL>\n"
